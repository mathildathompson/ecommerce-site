<h3 class="text-center">Products for Sale</h3>



 <div class="allProducts_container">
   <% @products.each do |product| %>
   <div class="indivi_product" id="draggable">
     <%#= image_tag product.img %>
      <%= product.name %>
      <%= number_to_currency product.price %>GBP
      <%= render :partial => 'product_info', :locals => {product:product} %>
    </div>
  <% end %>
</div>
